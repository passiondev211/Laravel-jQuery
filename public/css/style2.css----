.flyout-wrap .ani-expand 0% {
  top: 150px;
}

.flyout-wrap .ani-expand 50% {
  top: -10px;
}

.flyout-wrap .ani-expand 70% {
  top: 15px;
}

.flyout-wrap .ani-expand 100% {
  top: 10px;
}

.flyout-wrap .ani-contract 0% {
  top: 10px;
}

.flyout-wrap .ani-contract 40% {
  top: -25px;
}

.flyout-wrap .ani-contract 100% {
  top: 150px;
}

.flyout-wrap .ani-clicked 0% {
  opacity: 1;
  transform: scale(1);
}

.flyout-wrap .ani-clicked 100% {
  opacity: 0;
  transform: scale(5);
}

@keyframes expand {
  0% {
    top: 150px;
  }

  50% {
    top: -10px;
  }

  70% {
    top: 15px;
  }

  100% {
    top: 10px;
  }
}

@keyframes contract {
  0% {
    top: 10px;
  }

  40% {
    top: -25px;
  }

  100% {
    top: 150px;
  }
}

@keyframes clicked {
  0% {
    transform: scale(1);
    opacity: 1;
    top: 10px;
  }

  90% {
    top: 10px;
  }

  99% {
    transform: scale(6);
    opacity: 0;
    top: 150px;
  }

  100% {
    transform: scale(0);
  }
}

@keyframes fade {
  0% {
    transform: scale(1);
    opacity: 1;
    top: 10px;
  }

  90% {
    opacity: 0;
    top: 10px;
  }

  99% {
    transform: scale(0);
    top: 150px;
  }

  100% {
    transform: scale(0);
  }
}

